        {
          "datasource": {
            "type": "influxdb",
            "uid": "${DS_INFLUXDB}"
          },
          "hide": false,
          "query": "from(bucket: \"Polipharm\")\r\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"device_frmpayload_data_value_1\" or \r\n                        r[\"_measurement\"] == \"device_frmpayload_data_value_2\" or \r\n                        r[\"_measurement\"] == \"device_frmpayload_data_value_3\" or \r\n                        r[\"_measurement\"] == \"device_frmpayload_data_value_4\")\r\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\r\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)\r\n  |> map(fn: (r) => ({ \r\n        r with \r\n        _field: if r._measurement == \"device_frmpayload_data_value_1\" then \"พลังงานไฟฟ้าสะสม L1 (kWh)\"\r\n                else if r._measurement == \"device_frmpayload_data_value_2\" then \"พลังงานไฟฟ้าสะสม L2 (kWh)\"\r\n                else if r._measurement == \"device_frmpayload_data_value_3\" then \"พลังงานไฟฟ้าสะสม L3 (kWh)\"\r\n                else if r._measurement == \"device_frmpayload_data_value_4\" then \"พลังงานไฟฟ้าสะสม (kWh)\"\r\n                else r._field \r\n  }))\r\n  |> keep(columns: [\"_time\", \"_value\", \"_field\"])  // แสดงเฉพาะคอลัมน์ที่ต้องการ\r\n  |> yield(name: \"mean\")\r\n",
          "refId": "A"
        }
      ],
      "type": "timeseries"
    },
